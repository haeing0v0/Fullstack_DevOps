package cloud;

public class CharVariable1 {
	public static void main(String[] args) {
		
		char ch1 = 'A'; //65
		int ch2 = 97; //a
		
		System.out.println("ch1 = " + ch1);
		System.out.println("(int)ch1 = " + (int)ch1);
		
		System.out.println("ch2 = " + ch2);
		System.out.println("(char)ch2 = " + (char)ch2);
	}
}
