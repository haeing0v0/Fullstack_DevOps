package cloud;

public class forth {
	public static void main(String ar[]) {
		 int bnum = 0b00000000000000000101; //00000000
		 int bnum1 = 0b1111111111111111111111111011; //11111111
		 int sum = bnum + bnum1;
		 
		 System.out.println("bnum = " + bnum + ", bnum = " + bnum1);
		 System.out.println("sum = " + sum);
	}
}
